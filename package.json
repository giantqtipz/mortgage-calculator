{
  "name": "mortgage-calculator",
  "version": "1.0.0",
  "description": "Mortgage Calculator built with Express, Node.js, PostgreSQL, React, Typescript",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "build": "npm run compile && webpack --mode=\"production\"",
    "server": "node server/index.js",
    "start": "node server/index.js",
    "start:dev-build": "npm run compile:watch & webpack --mode=\"development\" --watch",
    "start:dev-server": "node-dev --respawn server/index.js",
    "syncDB": "node server/db/syncDB",
    "fred": "node ./server/api/utils/fred.js",
    "test": "jest --config ./jest.unit.json --coverage --detectOpenHandles"
  },
  "author": "Abimanyu Saridjo",
  "license": "ISC",
