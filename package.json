    "axios": "^0.21.4",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^5.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "file-saver": "^2.0.5",
    "fred-api": "^2.0.1",
    "node-fred": "^2.1.6",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.51.0",
    "sequelize": "^6.19.0",
    "xlsx": "^0.18.5"
  },
